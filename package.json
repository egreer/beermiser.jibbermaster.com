    "husky": "^9.1.5",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "pretty-quick": "^4.0.0",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "vite": "^5.4.6",
    "vite-plugin-pwa": "^0.20.2"
