{
  "name": "beermiser.jibbermaster.com",
  "version": "0.1.0",
  "private": true,
  "description": "A price and alcohol content calculator for the budget conscious drinker.",
  "keywords": [
    "beer",
    "budget",
    "calculator",
    "pricing"
  ],
  "homepage": "https://beermiser.jibbermaster.com",
  "bugs": {
    "url": "https://github.com/egreer/beermiser.jibbermaster.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egreer/beermiser.jibbermaster.com.git"
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://www.paypal.me/egreerme"
    }
  ],
  "license": "ISC",
  "author": "egreer",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky",
    "prettier": "prettier --cache --write '**/*.{ts,js,tsx,jsx,css,scss,html}'",
    "prettier:check": "prettier --check --write '**/*.{ts,js,tsx,jsx,css,scss,html}'",
    "prettier:nocache": "prettier --write '**/*.{ts,js,tsx,jsx,css,scss,html}'",
    "pretty-quick": "pretty-quick",
    "pretty-quick:staged": "pretty-quick --staged --pattern 'src/**/*.{ts,js,tsx,jsx,css,scss}'",
    "serve": "vite preview",
    "start": "vite",
    "update-packages": "npx npm-check-updates -u"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "bootstrap": "^5.3.8",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "react": "^19.2.3",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "react-router-dom": "^7.11.0",
    "store": "^2.0.12",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/store": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "pretty-quick": "^4.2.2",
    "sass": "^1.94.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.1.0"
  },
  "overrides": {
    "eslint-plugin-react-hooks": {
      "eslint": "^9.8.0"
    }
  }
}
