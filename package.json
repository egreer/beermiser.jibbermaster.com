    "pretty-quick": "pretty-quick",
    "pretty-quick:staged": "pretty-quick --staged --pattern 'src/**/*.{ts,js,tsx,jsx,css,scss}'",
    "serve": "vite preview",
    "start": "vite",
    "update-packages": "npx npm-check-updates -u"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
